/*
 * A variation of Donald Knuth's Man or Boy test
 */

fun a(k:int, x1:int, x2:int, x3:int, x4:int, x5:int):int {
    fun b():int {
        fun c():int {
            return a(k, x1, x2, x3, x4, x5);
        };

        k := k - 1;
        return a(k, c(), x1, x2, x3, x4);
    };
    var result:int;
    if (k =< 0) then {
        result := x4 + x5;
    } else {
        result := b();
    };
    return result;
};

var i:int;

i := 0;

while i < 8 do {
    print a(i, 1, -1, -1, 1, 0);
    i := i + 1;
};
